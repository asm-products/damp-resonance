<h3>Ad</h3>
<p id="notice"><%= notice %></p>

<%= link_to 'Edit', edit_ad_path(@ad) %> |
<%= link_to 'Back', ads_path %>
<p>Title: <%= @ad.title %></p>
<p>Description: <%= @ad.description %></p>
<% unless @ad.pic_url.nil? %><%= image_tag @ad.pic_url %><br /><% end %>
<%= image_tag "http://maps.google.com/maps/api/staticmap?size=225x150&sensor=false&zoom=10&markers=#{@ad.latitude}%2c#{@ad.longitude}" %>